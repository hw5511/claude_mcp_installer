{\n  \"name\": \"HTML to Notion MCP\",\n  \"version\": \"1.0.0\",\n  \"description\": \"Convert Claude-generated HTML to GitHub Gist and embed in Notion\",\n  \"author\": \"Claude Developer\",\n  \"requires_authentication\": true,\n  \"auth_info\": [\n    {\n      \"key\": \"GITHUB_TOKEN\",\n      \"name\": \"GitHub Personal Access Token\",\n      \"description\": \"GitHub API token with gist permission\",\n      \"required\": true\n    },\n    {\n      \"key\": \"GITHUB_USERNAME\",\n      \"name\": \"GitHub Username\",\n      \"description\": \"Your GitHub username\",\n      \"required\": true\n    }\n  ],\n  \"auth_guide\": {\n    \"title\": \"GitHub Personal Access Token Setup\",\n    \"steps\": [\n      \"Log in to your GitHub account\",\n      \"Go to Settings > Developer Settings > Personal access tokens > Tokens (classic)\",\n      \"Click Generate new token and select Generate new token (classic)\",\n      \"Name your token and select the gist permission\",\n      \"Click Generate token and copy your token\",\n      \"Enter the token during the MCP installation process\"\n    ]\n  },\n  \"requires_dependencies\": true,\n  \"dependencies\": {\n    \"python\": [\"requests\", \"fastmcp\"]\n  }\n} 
